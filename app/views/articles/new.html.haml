.container
  = form_with model: @article, local: true do |f|
    %form.col.s12
      .row
        .input-field.col.s6
          =f.text_field :title, class: "validate", id:"article_title", placeholder:"title"
    %form.col.s12
      .row
        .input-field.col.s12
          =f.text_area :text, class: "materialize-textarea", id:"textarea1", placeholder:"textarea"

